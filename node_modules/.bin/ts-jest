#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_afc576388baec5eec4b5dbea93bf2801/node_modules/ts-jest/node_modules:/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_afc576388baec5eec4b5dbea93bf2801/node_modules:/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_afc576388baec5eec4b5dbea93bf2801/node_modules/ts-jest/node_modules:/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_afc576388baec5eec4b5dbea93bf2801/node_modules:/home/maina/Desktop/Projects/RESTAURANT-API/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
